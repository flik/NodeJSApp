<%- include layouts/header.ejs %>

        <div class="page-data">
         <div class="data-btn">
           <button onClick="addMethod();">+ Add</button>
           <a class="a-inside edit" href="../">Home</a>
         </div>
         <div class="data-table">
            <table border="1" cellpadding="7" cellspacing="7">
                <tr>
                    <th width="50px">No</th>
                    <th>Payment Method Name</th>
                    <th>Key</th>
                    <th width="120px">Action</th>
                </tr>
                               
                <% if(data.length){ 
                                
                 for(var i = 0;i < data.length;i++) { %>
                 
                <tr>
                    <td><%=(i+1)%></td>
                    <td><%=data[i].paymentapi%></td>
                    <td><%=data[i].key%></td>
                     
                    <td>
                        <a class="a-inside edit" href="../methods/edit/<%=data[i].id%>">Edit</a>
                       
                        <a class="a-inside delete" href="../methods/delete/<%=data[i].id%>">Delete</a>
                       
                    </td>
                </tr>
            <% }
            
             }else{ %>
                 <tr>
                    <td colspan="4">No user</td>
                 </tr>
            <% } %>
                
                              
                
            </table>
         </div>
        </div>
        
<%- include layouts/footer.ejs %>
